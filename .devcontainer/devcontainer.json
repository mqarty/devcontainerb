{
  "name": "⚡ ⚡ devcontainerb ⚡ ⚡",
  "build": {
    "dockerfile": "./Dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "mounts": [
    "source=${localEnv:HOME}/.zsh_history,target=/root/.zsh_history,type=bind",
    "source=aws-sso-cache,target=/root/.aws/sso,type=volume",
    "source=${localWorkspaceFolder}/.devcontainer,target=/workspaces/b6n/.devcontainer,type=bind",
    "source=${localWorkspaceFolder}/notebooks,target=/workspaces/b6n/notebooks,type=bind"
  ],
  "runArgs": [],
  "customizations": {
    "vscode": {
      "extensions": [],
      "settings": {
        "editor.tabSize": 4,
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.profiles.linux": {
          "zsh": {
            "path": "zsh"
          }
        }
      }
    }
  },
  "containerEnv": {
    "HISTFILE": "/root/.zsh_history",
    "HISTSIZE": "100000",
    "SAVEHIST": "100000"
  },
  "postCreateCommand": "/workspaces/b6n/.devcontainer/setup-git-safe.sh"
}